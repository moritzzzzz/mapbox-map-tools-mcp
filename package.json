{
  "name": "mapbox-map-tools-mcp",
  "version": "1.0.1",
  "description": "A JavaScript library providing MCP-compatible tools for Mapbox GL JS map visualization with LLM function calling support",
  "main": "src/mapbox-map-tools.js",
  "module": "src/mapbox-map-tools.esm.js",
  "browser": "src/mapbox-map-tools.js",
  "types": "dist/mapbox-map-tools.d.ts",
  "files": [
    "dist/",
    "src/",
    "examples/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "dev": "rollup -c --watch",
    "serve": "http-server examples -p 8080",
    "test": "echo \"Tests not implemented yet\" && exit 0"
  },
  "keywords": [
    "mapbox",
    "mapbox-gl-js",
    "visualization",
    "geospatial",
    "maps",
    "llm",
    "mcp",
    "model-context-protocol",
    "function-calling",
    "tools",
    "gis"
  ],
  "author": "Moritz Forster",
  "license": "MIT",
  "peerDependencies": {
    "mapbox-gl": "^2.0.0 || ^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "http-server": "^14.1.1",
    "rollup": "^4.9.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moritzzzzz/mapbox-map-tools-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/moritzzzzz/mapbox-map-tools-mcp/issues"
  },
  "homepage": "https://github.com/moritzzzzz/mapbox-map-tools-mcp#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "directories": {
    "example": "examples"
  }
}
